x) => {\n                            return (\n                                <tr key={index}>\n                                    <td\n                                        className='cursor'\n                                        onClick={() => {\n                                            handleClickRow(item._id)\n                                        }}\n                                    >\n                                        {item.name}\n                                    </td>\n                                    <td\n                                        className='cursor'\n                                        onClick={() => {\n                                            handleClickRow(item._id)\n                                        }}\n                                    >\n                                        {item.description}\n                                    </td>\n                                    <td\n                                        className='cursor'\n                                        onClick={() => {\n                                            handleClickRow(item._id)\n                                        }}\n                                    >\n                                        {item.phone}\n                                    </td>\n                                    <td\n                                        className='cursor'\n                                        onClick={() => {\n                                            handleClickRow(item._id)\n                                        }}\n                                    >\n                                        {item.address}\n                                    </td>\n                                    <td>\n                                        <div className='buttons are-small'>\n                                            <button className='button is-warning'\n                                                onClick={() => {\n                                                    openModal(item)\n                                                }}\n                                            >\n                                                แก้ไข\n                                            </button>\n                                            <button className='button is-danger '\n                                                onClick={() => {\n                                                    handleDelete(item._id)\n                                                }}\n                                            >\n                                                ลบ\n                                            </button>\n                                        </div>\n                                    </td>\n                                </tr>\n                            )\n                        })\n                    }\n                </tbody>\n            </table>\n        </>\n    )\n}\n\nexport default withRouter(StoreTable)"},"lineCount":null}},"error":null,"hash":"8ee3e7548776d1acb29f3eba9b086014","cacheData":{"env":{}}}